{
  "name": "DSPaths",
  "version": "0.1.0",
  "summary": "A simple path templater for iOS",
  "description": "A simple way to abbreviate/expand paths in iOS so that they may be saved in data stores such as CoreData.",
  "homepage": "https://github.com/dannys42/DSPaths",
  "license": "MIT",
  "authors": {
    "Danny Sung": "danny@dannysung.com"
  },
  "source": {
    "git": "https://github.com/dannys42/DSPaths.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "DSPaths": [
      "Pod/Assets/*.png"
    ]
  },
  "public_header_files": "Pod/Classes/**/*.h"
}
